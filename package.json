    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "json5": "^2.1.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.9.7",
    "postcss-modules": "^1.4.1",
    "typescript": "^2.9.2",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^12.0.2",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "dayjs": "^1.8.14",
