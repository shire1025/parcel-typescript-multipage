  "name": "parcel-typescript-multipage",
  "version": "1.0.0",
  "description": "TypeScript app with parcel bundler.",
  "main": "lib/parcel-typescript-multipage.js",
  "scripts": {
    "initByYarn": "yarn global add parcel-bundler",
    "initByNpm": "npm install -g parcel-bundler",
    "start": "parcel src/**/*.html --no-hmr --no-cache --no-source-maps",
    "build": "cross-env NODE_ENV=production parcel build src/**/*.html --public-url ./",
    "watch": "parcel watch src/**/*.html"
  },
  "devDependencies": {
    "@babel/parser": "^7.4.5",
    "@types/es6-promise": "^3.3.0",
    "@types/lodash": "^4.14.132",
    "@types/qs": "^6.5.3",
